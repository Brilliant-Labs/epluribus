<% print_job = print_job_activity %>
<div class="row">
<% if ! print_job.user_id.nil? %>
  <div class="col-md-2">
    <%= image_tag print_job.user.avatar, width: 50 %>
  </div>
  <div class="col-md-10">
    <%= print_job.user.name %> <%= print_job_activity(print_job).html_safe %> Part #<%= print_job.part_id %>
<% else %>
  <div class="col-md-12">
    New Part #<%= print_job.part_id %> added to <%= print_job.project.name %>
<% end %>
  <span><%= time_ago_in_words(print_job.updated_at) %> ago</span>
  </div>
</div>
